<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>PicNet.XGBoost</PackageId>
    <Version>0.2.2</Version>
    <Authors>Guido Tapia, Adam Dobrzycki</Authors>
    <Company></Company>
    <Description>.Net wrapper for the XGBoost machine learning library. Targeted for x64 and .NET Framework 4 and above, and .NET Standard 2.0 and above.</Description>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/PicNet/XGBoost.Net</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="lib\libxgboost.dll" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="lib\libxgboost.dll" />
  </ItemGroup>

</Project>
